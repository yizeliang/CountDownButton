<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="max_count" format="integer" />


    <attr name="has_prepare" format="boolean" />

    <attr name="normal_text" format="string" />
    <attr name="prepare_text" format="string" />
    <attr name="timer_text" format="string" />
    <attr name="end_text" format="string" />

    <attr name="normal_bg" format="reference" />
    <attr name="prepare_bg" format="reference" />
    <attr name="timer_bg" format="reference" />
    <attr name="end_bg" format="reference" />

    <attr name="normal_text_color" format="color" />
    <attr name="prepare_text_color" format="color" />
    <attr name="timer_text_color" format="color" />
    <attr name="end_text_color" format="color" />

    <declare-styleable name="CountDownButton">

        <attr name="max_count" />

        <attr name="has_prepare" />


        <attr name="normal_text" />
        <attr name="prepare_text" />
        <attr name="timer_text" />
        <attr name="end_text" />

        <attr name="normal_bg" />
        <attr name="prepare_bg" />
        <attr name="timer_bg" />
        <attr name="end_bg" />

        <attr name="normal_text_color" />
        <attr name="prepare_text_color" />
        <attr name="timer_text_color" />
        <attr name="end_text_color" />
    </declare-styleable>
</resources>